{
  "name": "online-survey",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build-github": "ng build --bh /online-survey/ -op docs --prod ",
    "test-dev": "ng test",
    "test": "ng test --single-run --code-coverage",
    "lint": "ng lint",
